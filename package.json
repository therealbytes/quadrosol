{
  "name": "quadrosol",
  "version": "0.0.1",
  "description": "A solidity quadtree implementation",
  "main": "index.js",
  "repository": "https://github.com/therealbytes/quadrosol.git",
  "author": "bytes <therealbyt3s@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "forge test -vv --match-path test/Test.sol",
    "benchmark": "forge test -vv --match-path test/Bench.sol --gas-report",
    "build": "forge build",
    "lint": "yarn prettier && yarn solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "tinygo": "mkdir -p ./engine/bin && tinygo build -o ./engine/bin/quaddb.wasm -target wasi -opt=2 ./engine/tinygo/quaddb.go",
    "geth": "mkdir -p ./build/bin && go build -o ./build/bin/geth ./engine/main.go",
    "docker": "docker build -t ghcr.io/therealbytes/concrete-quad:latest .",
    "test:concrete": "sh LiveTest.sh"
  },
  "devDependencies": {
    "@latticexyz/solecs": "^1.33.1",
    "@solidstate/contracts": "^0.0.52",
    "ds-test": "https://github.com/dapphub/ds-test.git#c7a36fb236f298e04edf28e2fee385b80f53945f",
    "forge-std": "^1.1.2",
    "pclib": "ssh://git@github.com/therealbytes/pclib#2f1b21b3130858ce7b0deb00d4dc50ad28c82061",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7"
  }
}
